<Window x:Class="CarShop.AddNewCarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarShop"
        mc:Ignorable="d"
        Title="Add new car" Height="442.437" Width="326.051">
    <Grid Margin="0,0,2,3">
        <ComboBox Name="cbMake" HorizontalAlignment="Left" Margin="34,24,0,0" VerticalAlignment="Top" Width="120" ItemsSource="{Binding}" SelectionChanged="CbMake_SelectionChanged"/>
    </Grid>
</Window>
